<div class="main user-show">
  <div class="container">
    <div class="user">
    <h1><%= @user.name %>のページ</h1>
    <p>メールアドレス: <%= @user.email %></p>
    <% if @user.image? %>
    <%= image_tag @user.image.url %>
    <% end %>
    <%= link_to("Edit", edit_user_path(@user)) %>
    </div>
  </div>
</div>
